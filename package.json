{
	"name": "@intellion/logos",
	"version": "1.0.9",
	"description": "",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:dev": "jest --watch",
		"test:cov": "jest --coverage",
		"test:ci": "jest --maxWorkers=2",
		"test:cov:ci": "jest --coverage --maxWorkers=2",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
		"prepare": "rimraf .husky && husky install && npx husky add .husky/pre-commit \"npm run pre-commit\"",
		"pre-commit": "npm run build && lint-staged"
	},
	"lint-staged": {
		"*.+(js|ts)": [
			"npm run lint",
			"git add"
		],
		"*.+(json|css|md)": [
			"prettier --write",
			"git add"
		]
	},
	"author": "",
	"license": "ISC",
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=6.14.12"
	},
	"directories": {
		"test": "test"
	},
	"dependencies": {
		"@intellion/arche": "^1.0.2",
		"@types/express": "^4.17.13",
		"axios": "^0.24.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"pg": "^8.7.1",
		"rimraf": "^3.0.2",
		"typeorm": "^0.2.41"
	},
	"devDependencies": {
		"@types/expect": "^24.3.0",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.3.2",
		"@types/pg": "^8.6.1",
		"@typescript-eslint/eslint-plugin": "^4.28.5",
		"@typescript-eslint/parser": "^4.28.5",
		"eslint": "^7.31.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^7.0.0",
		"jest": "^27.3.1",
		"lint-staged": "^11.1.1",
		"prettier": "^2.3.2",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.1.0",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	}
}
